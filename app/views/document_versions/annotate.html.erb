<%= @document_version.id %>
<%= @document_version.name %>

<br/><br/><br/>

<div id="image-area">
  <%= image_tag @document_version.file_url, id: "pdf" %>
</div>

<script>

  function blackNote() {
    return $(document.createElement('span')).addClass('black')
  }
    
  $('#pdf').annotatableImage(blackNote);

</script>